<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane stylesheets="@../resources/application.css" prefHeight="80.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="11.0" layoutY="14.0" text="Do you want to save before exiting?" />
      <HBox alignment="CENTER" layoutY="47.0" prefHeight="53.0" prefWidth="260.0">
         <children>
            <Button mnemonicParsing="false" onAction="#savePressed" text="Save" />
            <Button mnemonicParsing="false" onAction="#dontSavePressed" text="Don't Save">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#cancelPressed" text="Cancel" />
         </children>
      </HBox>
   </children>
</AnchorPane>
